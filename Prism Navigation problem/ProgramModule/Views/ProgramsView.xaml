<UserControl
    x:Class="ProgramModule.Views.ProgramsView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:ProgramModule.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="300"
    d:DesignWidth="300"
    mc:Ignorable="d">
    <StackPanel Background="Aquamarine">
        <ContentControl Content="{Binding Title}" />
        <Button Command="{Binding NavigateCommand}" Content="Aller vers Edit" />
    </StackPanel>
</UserControl>